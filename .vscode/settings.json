{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.options": {
    "extensions": [".js", ".json", ".md", ".mdx", ".ts", ".tsx"]
  },
  "eslint.validate": [
    "javascript",
    "json",
    "markdown",
    "mdx",
    "typescript",
    "typescriptreact"
  ],
  "eslint.packageManager": "pnpm",
  "cSpell.words": ["Arweave", "multistream"],
  "json.format.enable": false
}
